<view><view class="jiangqie-timeline-head"><block wx:if="{{setting.background&&setting.background.length>0}}"><image src="{{setting.background}}" mode="aspectFill"></image></block><block wx:if="{{setting.title.length>0||setting.description.length>0}}"><view class="timeline-head-text"><text>{{setting.title}}</text><view class="timeline-head-describe">{{setting.description}}</view></view></block></view><view style="{{(tl_background?'background: url('+tl_background+') repeat-y; background-position: -30rpx 0;':'')}}"><block wx:if="{{posts.length>0}}"><view class="jiangqie-timeline-view"><block wx:for="{{posts}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="jiangqie-timeline-block" data-id="{{item.id}}" data-event-opts="{{[['tap',[['handlerHotArticle',['$event']]]]]}}" catchtap="__e"><view class="jiangqie-timeline-times"><image src="/static/images/timelinedot.png" mode="aspectFill"></image><text>{{item.time}}</text></view><view class="jiangqie-timeline-box timeline-single" style="height:50rpx;"><view class="jiangqie-timeline-text"><view class="jiangqie-timeline-title">{{item.title}}</view></view></view></view></block><jiangqie-loadmore vue-id="0e4143a4-1" visible="{{loadding}}" bind:__l="__l"></jiangqie-loadmore><jiangqie-nomore vue-id="0e4143a4-2" visible="{{!pullUpOn}}" bind:__l="__l"></jiangqie-nomore></view></block><block wx:else><block wx:if="{{loaded}}"><jiangqie-no-data vue-id="0e4143a4-3" bind:__l="__l"></jiangqie-no-data></block></block></view><block wx:if="{{loading}}"><jiangqie-loading vue-id="0e4143a4-4" bind:__l="__l"></jiangqie-loading></block></view>